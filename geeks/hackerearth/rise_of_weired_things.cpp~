/*
 * July easy
 */

#include <iostream>
#include <algorithm>
#inlcude <vector>
using namespace std;

int main() {
  int n;
  cin >> n;
  if(n < 1 || n > 1000) {
    return 0;
  }

  vector<int> zombies = new vector<int>;
  vector<int> vamps = new vector<int>;

  for(int i = 0; i < n; ++i) {
    int k;
    cin >> k;
    if(k < 1 || k > 1000) {
      return 0;
    }
    else if(k%2 == 0) {
      zombies.push_back(n);
    }
    else {
      vamps.push_back(n);
    }
  }


}
