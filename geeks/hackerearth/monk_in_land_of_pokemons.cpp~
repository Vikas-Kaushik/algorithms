/*
 * code monk hashing challenge
 */
#include <iostream>
using namespace std;

int main() {
  size_t testCount;
  cin >> testCount;
   	
  if(testCount < 1 || testCount > 10)	return 0;
   		
  while(testCount--)	{
    size_t n;
    cin >> n;
    if(n < 1 || n > 100000) return 0;
   		
    while(n--) {
      size_t a,b;
      cin >> a >> b;
      if(a < 1 || a > 1000000 || b < 1 || b > 1000000) return 0;
   			

    cout << count;
  }
}
