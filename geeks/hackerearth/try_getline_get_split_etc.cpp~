#include <iostream>
#include <string>

using namespace std;
int main() {
  size_t testCount;
  if(testCount < 0 || testCount > 100) return 0;

  vector<int> input;
  int *iarry;
  for(int i = 0; i < testCount; ++i) {
    cout << i < " ";
    iarry = cin.getline();
    cout << iarry << endl
  }
  return 0;
}
