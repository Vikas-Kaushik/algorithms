#ifndef _singlylist_h
#define _singlylist_h

wstruct Node{
  int data;
  Node* next;
};

Node* makeList(int count, ...);
void addNode(Node** head, int data);
void deleteList(Node** head);
void displayList(Node *head);

/* Usage of liabray

struct Node{
  int data;
  Node* next;
};

int main(){
  Node* head = makeList(5,10,45,65,23,9);
  displayList(head);
  deleteList(&head);
  displayList(head);
  return 0;
}
*/

#endif
