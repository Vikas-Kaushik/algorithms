#include<iostream>
#include<cctype>

using namespace std;

bool EqualIgnoringCase(String str1, String str2);

int main(){

  return 0;
}

bool EqualIgnoringCase(String str1, String str2){
  int l1 = str1.length();
  int l2 = str2.length();

  if(l1 != l2)
    return false;

  for(int i = 0; i < l1; ++i){
    if( (toupper(str[i])) != (tolower(str[i])) ){
	return false;
    }
  }
  return true;
}
